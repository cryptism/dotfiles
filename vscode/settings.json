{
    "background.enabled": true,
    "background.useDefault": false,
    "background.customImages": [
        "~/dotfiles/vscode/backgrounds/1.png"
    ],
    "background.style": {
        "opacity": 0.2,
    },
    "typescript.validate.enable": false,
    "background.useFront": true,
    "breadcrumbs.enabled": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "window.zoomLevel": 1,
    "editor.fontFamily": "'Iosevka', 'Fira Code', 'Fira Mono', 'Courier New', monospace",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.files.associations": {
        "*.test.{js,jsx}": "test-tube"
    },
    "extensions.autoUpdate": true,
    "typescript.autoImportSuggestions.enabled": false,
    "typescript.format.enable": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            { 
                "scope": [
                    "comment",
                    "variable.language",
                    "punctuation.definition.comment",
                    "entity.name.method.js",
                    "text.html.basic entity.other.attribute-name.html",
                    "text.html.basic entity.other.attribute-name",
                    "tag.decorator.js entity.name.tag.js",
                    "tag.decorator.js punctuation.definition.tag.js",
                    "source.js constant.other.object.key.js string.unquoted.label.js",
                    "markup.italic",
                    "markup.quote"
                ],
                "settings": {"fontStyle": ""}
            }
        ]
    },
    "javascript.updateImportsOnFileMove.enabled": "never",
    "code-runner.executorMap": {
        "javascript": "babel-node"
    },
    "editor.parameterHints": true,
    "editor.fontSize": 13,
    "files.associations": {
        "*.mkd": "markdown"
    },
    "search.exclude": {
        "**/dist": true,
        "**/build": true
    },
    "files.insertFinalNewline": true,
    "eslint.autoFixOnSave": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "terminal.enableAppInsights": false,
    "journal.base": "~/Documents/Journal",
    "workbench.sideBar.location": "right",
    "diffEditor.renderSideBySide": false,
    "javascript.validate.enable": false,
    "files.autoSave": "afterDelay",
    "purescript.pscIdePort": 4242,
    "purescript.pscIdeServerExe": "psc-ide-server",
    "purescript.autoStartPscIde": true,
    "purescript.addNpmPath": true,
    "purescript.packagePath": "**/bower_components",
