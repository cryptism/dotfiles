{
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "window.zoomLevel": 0,
    "editor.fontFamily": "'Fira Code', 'Fira Mono', 'Courier New', monospace",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "material-icon-theme",
    "extensions.autoUpdate": true,
    "editor.parameterHints": true,
    "editor.fontSize": 13,
    "files.associations": {
        "*.mkd": "markdown"
    },
    "search.exclude": {
        "**/dist": true,
        "**/build": true
    },
    "files.insertFinalNewline": true,
    "vsicons.dontShowNewVersionMessage": true,
    "eslint.autoFixOnSave": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "terminal.enableAppInsights": false,
    "journal.base": "~/Documents/Journal",
    "workbench.sideBar.location": "right",
    "diffEditor.renderSideBySide": false,
    "javascript.validate.enable": false,
    "files.autoSave": "afterDelay",
    "purescript.pscIdePort": 4242,
    "purescript.pscIdeServerExe": "psc-ide-server",
    "purescript.autoStartPscIde": true,
    "purescript.addNpmPath": true,
    "purescript.packagePath": "**/bower_components",
